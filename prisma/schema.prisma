datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GameAccount {
  @@map("game_account")
  id         Int     @id @default(autoincrement())
  name       String
  nickName   String
  username   String
  password   String
  gameServer String
  online String
  userId Int
  level Int
}

model Device {
  @@map("device")
  id         Int     @id @default(autoincrement())
  imei       String
  name       String
  brand      String
  robotName   String
  robotId String
  status  String
  ip      String
  touchId String
  userId Int
  deviceType String
}

model Task {
  @@map("task")
  id         Int     @id @default(autoincrement())
  date       String
  name       String
  taskNo     String
  deviceId   Int
  accountId  Int
  startTime  Int
  updateTime Int
  endTime    Int
  status String
  note  String
  taskCount Int @default(0)
  income Int
  realIncome Int
  userId Int
  gameServer String
}

model TaskLog {
  @@map("task_log")
  id         Int     @id @default(autoincrement())
  imei       String
  nickName       String
  taskNo     String
  deviceId   Int
  accountId  Int
  taskName String
  note  String
  type String
  time Int
  taskCount Int
  userId Int
}

model Report {
  @@map("report")
  id         Int     @id @default(autoincrement())
  time       Int
  date       String
  type       String
  income     Float
  expend     Float
  note       String
  userId Int
  taskCount Int
  gameId String
  groupId Int
  profit Float
}

model User {
  @@map("user")
  id         Int     @id @default(autoincrement())
  username   String
  password   String
  vipCardId  Int
}

model VipCard {
  @@map("vip_card")
  id         Int     @id @default(autoincrement())
  level      Int
  createdTime   Int
  endTime  Int
}

model ConfigImage {
  @@map("config_image")
  id         Int     @id @default(autoincrement())
  name String
  path  String
  taskId Int
  deviceId Int
  url String
  userId Int
}

model GameGroup {
  @@map("game_group")
  id         Int     @id @default(autoincrement())
  name String
  userId  Int
  gameServer String
  type String
  priceConfig String
}

model GameRole {
  @@map("game_role")
  id         Int     @id @default(autoincrement())
  accoutId   Int
  userId  Int
  gameServer String
  work String
  name String
  gameId String
  status String
  groupId Int
  level Int
}

model UnloadDirective {
  @@map("unload_directive")
  id         Int     @id @default(autoincrement())
  code    String
  gameId  String
  status  String
  data    String
  config  String
  total   Int
  totalPrice Int
  createTime Int
  targetId String
  classifyNo String
}

