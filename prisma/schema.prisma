datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GameAccount {
  @@map("game_account")
  id         Int     @id @default(autoincrement())
  name       String
  username   String
  password   String
  gameServer String
  // content   String?
  // published Boolean @default(false)
  // author    User?   @relation(fields: [authorId], references: [id])
  // authorId  Int?
}

